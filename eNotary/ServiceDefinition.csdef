<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="eNotary" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
  <WebRole name="eNotaryWebRole" vmsize="Small" enableNativeCodeExecution="true">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="https" port="80" certificate="Microsoft.WindowsAzure.Plugins.RemoteAccess.PasswordEncryption" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
      <Import moduleName="Connect" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="EmailAdmin" />
      <Setting name="eNotarySpace" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="eNotarySpace" cleanOnRoleRecycle="false" sizeInMB="30000" />
    </LocalResources>
    <Certificates>
    </Certificates>
  </WebRole>
</ServiceDefinition>